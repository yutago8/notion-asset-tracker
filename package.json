{
  "name": "notion-asset-tracker",
  "version": "0.1.0",
  "private": true,
  "description": "Track daily portfolio value with Notion integration.",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "snapshot": "ts-node src/scripts/daily-snapshot.ts",
    "inspect:page": "ts-node src/scripts/inspect-page.ts",
    "introspect": "ts-node src/scripts/introspect-notion.ts",
    "lint": "eslint . || echo 'lint skipped'"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "axios": "^1.7.0",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "eslint": "^8.57.0"
  }
}
